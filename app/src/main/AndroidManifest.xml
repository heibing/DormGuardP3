<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lars.dormguardp3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LiveFeed"
            android:label="@string/title_activity_live_feed"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VideoLibrary"
            android:label="@string/title_activity_video_library"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VideoLibraryPlayer"
            android:label="@string/title_activity_video_library_player"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UserAccess"
            android:label="@string/title_activity_user_access"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
